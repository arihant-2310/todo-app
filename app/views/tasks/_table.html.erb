<table class="table mb-5" id="task-table-view">
  <thead>
  <tr>
    <th scope="col">Name</th>
    <th scope="col">Status</th>
    <th scope="col">Action</th>
  </tr>
  </thead>
  <tbody>
  <% tasks.each do |task| %>
    <tr>
      <td><%= task.name %></td>
      <td><%= task.status %></td>
      <td> <%= link_to "View", task, class: "btn btn-outline-success" %></td>
    </tr>
  <% end %>
  </tbody>
</table>
